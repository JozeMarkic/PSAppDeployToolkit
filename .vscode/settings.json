{
    "files.exclude": {
        ".exe":true,
        ".msi":true,
        ".editorconfig": true,
        ".gitattributes": true,
        ".github": true,
        "**/Deploy-Application.exe": true,
        "**/Deploy-Application.exe.config": true,
        "AppDeployToolkitLogo.png": true,
        "CHANGELOG.txt": true,
        "COPYING.Lesser": true,
        "docs": true,
        "Examples": true,
        "LICENSE": true,
        "material": true,
        "mkdocs.yml": true,
        "PSAppDeployToolkit.pdf": true,
        "README.md": true,
        "Sources": true,
        "Testing": true,
        "TODO.txt": true,
        "wiki": true
    },
    "terminal.integrated.profiles.windows": {
        "PowerShell DevOps": {
            "path": "C:\\Windows\\System32\\WindowsPowerShell\\v1.0\\powershell.exe",
            "source": "PowerShell",
            "icon": "terminal-powershell",
            "args": [
                "-NoLogo",
                "-NoExit",
                "-ExecutionPolicy",
                "RemoteSigned",
                "Import-Module -Name PSReadLine; Clear-Host"

            ]
        },
        "Command Prompt": {
            "path": [
                "${env:windir}\\Sysnative\\cmd.exe",
                "${env:windir}\\System32\\cmd.exe"
            ],
            "args": [],
            "icon": "terminal-cmd"
        },
        "Git Bash": null,
        "Azure Cloud Shell (Bash)": null,
        "JavaScript Debug Terminal": null,
    },
    "terminal.integrated.defaultProfile.windows": "PowerShell DevOps",
    "files.defaultLanguage": "powershell",
    "terminal.integrated.enablePersistentSessions": false,
    "terminal.integrated.persistentSessionReviveProcess": "never",
    "powershell.scriptAnalysis.settingsPath": ".vscode\\PSScriptAnalyzerSettings.psd1"
}